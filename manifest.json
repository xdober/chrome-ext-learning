{
	"manifest_version": 2,

	"name": "Screen Time",
	"description": "This extension display time on screen.",
	"version": "1.0",
	"background": {"scripts": ["js/background.js"]},
	"icons": {
		"128": "imgs/icon128.png"
	} ,
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["js/clk.js"],
			"css":["CSS/clkstyle.css"]
		}
	],
	"permissions":[
		"tabs", "<all_urls>"
	],
	"browser_action": {
		"default_action": {
			"19": "imgs/icon128.png"
		},
		"default_popup": "popup.html"
	}
}