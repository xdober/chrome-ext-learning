{
	"manifest_version": 2,

	"name": "One-click Kittens",
	"description": "This extension demostrates a browser action with kittens.",
	"version": "1.0",
	"icons": {
		"16": "icon.png"
		} ,

	"permissions": [
		"https://secure.flickr.com/"
	],
	"browser_action": {
		"default_action": {
			"19": "icon.png"
		},
		"default_popup": "popup.html"
	}
}